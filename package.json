{
  "name": "metadock",
  "version": "1.3.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/blocksecteam/metadock.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "fix:style": "stylelint --fix src/**/*.{css,sass,scss,less,vue}",
    "lint:style": "stylelint src/**/*.{css,sass,scss,less,vue}",
    "prepare": "husky install && husky set .husky/pre-commit \"npx lint-staged\" && husky set .husky/commit-msg 'npx --no -- commitlint --edit \"$1\"'",
    "lint": "eslint 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "antd": "^5.0.2",
    "big.js": "^6.2.1",
    "buffer": "^6.0.3",
    "chrome-extension-core": "^0.1.4",
    "classnames": "^2.3.2",
    "copy-to-clipboard": "^3.3.3",
    "d3-graphviz": "2.6.1",
    "dayjs": "^1.11.6",
    "ethers": "^5.7.2",
    "i18next": "^22.0.4",
    "ky": "^0.32.2",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@crxjs/vite-plugin": "2.0.0-beta.5",
